@model OnlineQuizApp.Models.QuizResponse

<h2>QuizPage</h2>
@Html.Raw(ViewBag.question)
<br />
@ViewBag.qNum

<table>
    <tr>
        @for(int i = 0; i < 10; i++)
        {
            <td>
                <a href="@Url.Action("QuizPage", "Home", new { @token = Session["TOKEN"], @qNum = (i+1) } )">@(i+1)</a>
            </td>
        }
    </tr>
</table>

